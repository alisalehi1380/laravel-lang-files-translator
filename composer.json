{
  "name": "alisalehi/laravel-lang-files-translator",
  "description": "easiest way to translate lang files to another language in laravel",
  "keywords": [
    "laravel lang files translator",
    "laravel translator"
  ],
  "homepage": "https://github.com/alisalehi1380/laravel-lang-files-translator",
  "license": "MIT",
  "authors": [
    {
      "name": ":Ali Salehi",
      "email": "alisalehi13800930@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "stichoza/google-translate-php": "^5.1",
    "laravel/framework": "9.*|10.*|11.*"
  },
  "autoload": {
    "psr-4": {
      "Alisalehi\\LaravelLangFilesTranslator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Alisalehi\\LaravelLangFilesTranslator\\LangFilesTranslatorServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.4"
  },
  "minimum-stability": "stable"
}
