{
    "name": "alisalehi/laravel-translator-lang-files",
    "description": "easiest way to translate lang files to another language in laravel",
    "keywords": [
        "laravel translator lang files",
        "translator lang files"
    ],
    "homepage": "https://github.com/alisalehi1380/laravel-translator-lang-files",
    "license": "MIT",
    "authors": [
        {
            "name": ":Ali Salehi",
            "email": "alisalehi13800930@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "illuminate/contracts": "9.*|10.*",
        "stichoza/google-translate-php": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Alisalehi\\LaravelTranslatorLangFiles\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alisalehi\\LaravelTranslatorLangFiles\\LaravelTranslatorLangFilesServiceProvider"
            ]
        }
    }
}